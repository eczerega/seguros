<h1>Listing stores_users</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores_users.each do |stores_user| %>
      <tr>
        <td><%= stores_user.user_id %></td>
        <td><%= stores_user.store_id %></td>
        <td><%= link_to 'Show', stores_user %></td>
        <td><%= link_to 'Edit', edit_stores_user_path(stores_user) %></td>
        <td><%= link_to 'Destroy', stores_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stores user', new_stores_user_path %>
