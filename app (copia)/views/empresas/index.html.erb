<h1>Listing empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Digito verificador</th>
      <th>Giro</th>
      <th>Domicilio comercial</th>
      <th>Representante legal</th>
      <th>Mail</th>
      <th>Telefono</th>
      <th>Cuenta bancria</th>
      <th>Banco</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nombre %></td>
        <td><%= empresa.rut %></td>
        <td><%= empresa.digito_verificador %></td>
        <td><%= empresa.giro %></td>
        <td><%= empresa.domicilio_comercial %></td>
        <td><%= empresa.representante_legal %></td>
        <td><%= empresa.mail %></td>
        <td><%= empresa.telefono %></td>
        <td><%= empresa.cuenta_bancria %></td>
        <td><%= empresa.banco %></td>
        <td><%= empresa.estado %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
